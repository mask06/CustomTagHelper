@{
    var users = ViewData["users"] as List<User>;
}
<table>
    <tr>

        <th>ID</th>
        <th>Isım</th>
        <th>Email</th>
        <th>--</th>
    </tr>
    @foreach (var user in users)
    {
        <tr>
            <td>
                @user.Id
            </td>
            <td>
                @user.Name
            </td>
            <td>
                @user.Email
            </td>
        </tr>
    }
</table>